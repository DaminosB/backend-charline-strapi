{
  "collectionName": "components_module_thumbnails",
  "info": {
    "displayName": "Vignettes",
    "icon": "apps",
    "description": ""
  },
  "options": {},
  "attributes": {
    "medias": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "backgroundColor": {
      "type": "customField",
      "regex": "^#([A-Fa-f0-9]{6}|[A-Fa-f0-9]{3})$",
      "required": true,
      "customField": "plugin::color-picker.color",
      "default": "#FFFFFF"
    },
    "imagesPerRow": {
      "type": "integer"
    },
    "backgroundImage": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    }
  }
}
